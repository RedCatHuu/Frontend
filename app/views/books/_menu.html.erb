<div class="head d-flex">
  <%= image_tag "icon/logo.svg" %>
  <p>ServecName</p>
</div>

<div class="col">
  <table>
    <% books.each do |book| %>
      <tr class="cards">
        <td class="menu-title">
          <%= link_to main_path(:book_id => book.id), remote: true do %>
            <%= book.title %>
          <% end %>
        </td>
      </tr>
    <% end %>
  </table>
</div>

<div class="foot">
  <%= render 'shared/edit_button', target_path: menu_edit_path %>
</div>