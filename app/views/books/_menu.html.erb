<div class="head d-flex">
  <%= image_tag "icon/logo.svg" %>
  <p>ServecName</p>
</div>

<div class="col">
  <ul>
    <% books.each do |book| %>
      <li class="cards">
        <%= link_to book_path(book) do %>
          <%= book.title %>
        <% end %>
      </li>
    <% end %>
    
  </ul>
      
  
</div>

<div class="foot">
  <div class="main-edit-btn">
    <%= image_tag "icon/edit.svg", size:"18x18" %>
    <p class="main-edit-text">Edit</p>
  </div>
</div>